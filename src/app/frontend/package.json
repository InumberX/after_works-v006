    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.9.7",
    "jotai": "^2.12.3",
    "next": "15.3.1",
    "next-international": "^1.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.56.2",
    "string-width": "7.2.0",
    "strip-ansi": "7.1.0",
    "wrap-ansi": "9.0.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
