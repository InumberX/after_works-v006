{
  "name": "afterworks",
  "version": "6.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-quiet": "next lint --quiet",
    "stylelint": "stylelint 'src/**/*.{css,scss,sass}'",
    "stylelint-fix": "stylelint --fix 'src/**/*.{css,scss,sass}'",
    "format": "prettier --check 'src/**/*.{js,jsx,ts,tsx}'",
    "format-fix": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "typecheck": "tsc -b",
    "test": "vitest",
    "storybook": "storybook dev --no-open -p 6006 -c .storybook",
    "storybook-build": "storybook build -c .storybook",
    "upgrade": "npx npm-check-updates -u",
    "upgrade-check": "npx npm-check-updates",
    "pre-commit": "npm run typecheck && npm run lint && npm run stylelint-fix && npm run format-fix"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@sentry/nextjs": "^9.15.0",
    "@splidejs/react-splide": "^0.7.12",
